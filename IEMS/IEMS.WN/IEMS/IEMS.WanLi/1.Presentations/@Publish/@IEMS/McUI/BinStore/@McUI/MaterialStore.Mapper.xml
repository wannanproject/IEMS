<sqlMap namespace="IEMS.Frame.DbCI.McUIService"
	xmlns="http://ibatis.apache.org/mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
   <statements>
    <select id="Select@MaterialStore" parameterClass="map" resultClass="row">
      <![CDATA[ 
        SELECT MATERIAL_NO,COUNT(*) AS ACOUNT FROM WBS_BIN T
          GROUP BY MATERIAL_NO
      ]]>
     </select>
  </statements>
</sqlMap>
