<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="IEMS.Frame.DbCI.McUIService"
	xmlns="http://ibatis.apache.org/mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
   <statements>
    <select id="Select@View_ViewAgvstatus" parameterClass="map" resultClass="row">
      <![CDATA[ 
       SELECT * FROM VIEW_AGVSTATUS T1
      ]]>
      <dynamic prepend="WHERE">
         <isNotNull property="where.AGV_NO" prepend="AND">
           T1.AGV_NO LIKE '%'||#where.AGV_NO#||'%'
         </isNotNull>
       </dynamic>
       ORDER BY AGV_NO
     </select>
     <select id="Select@View_ViewAgvstatus@MainDetail" parameterClass="map" resultClass="row">
       <![CDATA[ 
       SELECT * FROM VIEW_AGVSTATUS T1
      ]]>
       <dynamic prepend="WHERE">
         <isNotNull property="where.AGV_NO" prepend = "AND">
           T1.AGV_NO = #where.AGV_NO#
         </isNotNull>
       </dynamic>
     </select>
  </statements>
</sqlMap>
