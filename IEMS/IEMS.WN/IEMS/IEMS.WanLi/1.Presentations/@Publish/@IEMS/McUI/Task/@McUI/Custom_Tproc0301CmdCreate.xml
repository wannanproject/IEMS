  <WebPage Title="生成指令过程">
  </WebPage>
  <Select>
    <ParamPanel>
      <ParamFields>
        <ParamField FieldName="TASK_NO"></ParamField>
        <ParamField FieldName="CMD_OBJID"></ParamField>
        <ParamField FieldName="TRANS_NO"></ParamField>
        <ParamField FieldName="PALLET_NO"></ParamField>                    
      </ParamFields>
    </ParamPanel>
    <MainGrid>
      <GridColumns>       
        <GridColumn ColumnName="TASK_NO" ></GridColumn>
        <GridColumn ColumnName="CMD_OBJID" ></GridColumn>            
        <GridColumn ColumnName="PALLET_NO" ></GridColumn>
        <GridColumn ColumnName="TRANS_NO" ></GridColumn>
             
        <GridColumn ColumnName="PROC_STATUS" ></GridColumn>        
        <GridColumn ColumnName="ROUTE_NO" ></GridColumn>
        
        <GridColumn ColumnName="SLOC_NO" ></GridColumn>
