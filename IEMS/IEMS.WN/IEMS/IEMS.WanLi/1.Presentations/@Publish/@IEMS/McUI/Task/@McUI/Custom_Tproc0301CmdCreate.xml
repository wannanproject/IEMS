    <MainDetail>
      <ParamFields>
        <ParamField FieldName="ERR_CODE" ></ParamField>
        <ParamField FieldName="ERR_DESC" ></ParamField>                
        <ParamField FieldName="TASK_GUID" ></ParamField>
