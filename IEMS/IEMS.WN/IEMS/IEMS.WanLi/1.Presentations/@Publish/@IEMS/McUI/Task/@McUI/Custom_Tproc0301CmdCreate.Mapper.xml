<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="IEMS.Frame.DbCI.McUIService"
	xmlns="http://ibatis.apache.org/mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  
   <statements>
    <select id="Select@Custom_Tproc0301CmdCreate" parameterClass="map" resultClass="row">
      <![CDATA[ 
      select t.*
      from TPROC_0301_CMD_CREATE t
      ]]>
      <dynamic prepend="where">      
        <isNotNull property="where.TASK_NO" prepend="AND">
          T.TASK_NO LIKE '%'||#where.TASK_NO#||'%'
        </isNotNull>
        <isNotNull property="where.PALLET_NO" prepend="AND">
          T.PALLET_NO LIKE '%'||#where.PALLET_NO#||'%'
        </isNotNull>
        <isNotNull property="where.TRANS_NO" prepend="AND">
          T.TRANS_NO LIKE '%'||#where.TRANS_NO#||'%'
        </isNotNull>
        <isNotNull property="where.CMD_OBJID" prepend="AND">
          T.CMD_OBJID LIKE '%'||#where.CMD_OBJID#||'%'
        </isNotNull>       
       </dynamic>
      ORDER BY t.CMD_OBJID
    </select>
     <select id="Select@Custom_Tproc0301CmdCreate@MainDetail" parameterClass="map" resultClass="row">
       <![CDATA[ 
       SELECT * FROM TPROC_0301_CMD_CREATE T1
      ]]>
       <dynamic prepend="WHERE">
         <isNotNull property="where.OBJID" prepend = "AND">
           T1.OBJID = #where.OBJID#
         </isNotNull>
       </dynamic>
     </select>
  </statements>
</sqlMap>
