      <GridColumns>        
        <GridColumn ColumnName="MATER_NO" WIDTH="180"></GridColumn>       
      </GridColumns>
    </DetailGrid>
    <MainDetail>
      <ParamFields>
        <ParamField FieldName="MATER_NO" ></ParamField>       
      </ParamFields>
