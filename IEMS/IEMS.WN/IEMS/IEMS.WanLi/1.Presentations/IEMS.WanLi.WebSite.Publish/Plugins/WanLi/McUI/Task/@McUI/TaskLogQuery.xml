<?xml version="1.0" encoding="utf-8" ?>
<UiConfig>
  <Captions>
    <Caption Name="TASK_NO"            Value="任务编号" ></Caption>
    <Caption Name="ORDER_NO"        Value="订单编号" ></Caption>
    <Caption Name="IO_TYPE"           Value="任务类型" ></Caption>
    <Caption Name="SLOC_NO"           Value="起始地址" ></Caption>
    <Caption Name="ELOC_NO"           Value="目标地址"></Caption>
    <Caption Name="PALLET_NO"           Value="工装号" ></Caption>
    <Caption Name="TASK_EXEC_START_DT"       Value="开始时间" ></Caption>
    <Caption Name="TASK_EXEC_END_DT"    Value="结束时间" ></Caption>
  </Captions>
  <WebPage Title="任务历史">
  </WebPage>
  <Select>
    <ParamPanel>
      <ParamFields>
        <ParamField FieldName="TASK_NO"></ParamField>
        <ParamField FieldName="ORDER_NO"></ParamField>
        <ParamField FieldName="IO_TYPE"></ParamField>
        <ParamField FieldName="SLOC_NO"></ParamField>
        <ParamField FieldName="ELOC_NO"></ParamField>
        <ParamField FieldName="PALLET_NO"></ParamField>
        <ParamField FieldName="TASK_EXEC_START_DT" Type="DateBox" DefaultValue="0"></ParamField>
        <ParamField FieldName="TASK_EXEC_END_DT" Type="DateBox" DefaultValue="0"></ParamField>
      </ParamFields>
    </ParamPanel>
    <MainGrid  PageSize ="50">
      <GridColumns>
        <GridColumn ColumnName="TASK_NO"            Width="60"></GridColumn>
        <GridColumn ColumnName="ORDER_NO"        Width="90"></GridColumn>
        <GridColumn ColumnName="IO_TYPE"           Width="150"></GridColumn>
        <GridColumn ColumnName="SLOC_NO"         Width="60"></GridColumn>
        <GridColumn ColumnName="ELOC_NO"        Width="60"></GridColumn>
        <GridColumn ColumnName="PALLET_NO"           Width="60"></GridColumn>
        <GridColumn ColumnName="TASK_EXEC_START_DT"       Width="140" ColumnType="Date" ColumnFormt="yyyy-MM-dd HH:mm:ss"></GridColumn>
        <GridColumn ColumnName="TASK_EXEC_END_DT"    Width="140" ColumnType="Date" ColumnFormt="yyyy-MM-dd HH:mm:ss"></GridColumn>
      </GridColumns>                      
    </MainGrid>                 
  </Select>
</UiConfig>